package	"Overlapper"
version	"1.0"
purpose	"Comparison of two answer files"

section "Main"
option	"first"		i	"First DAB file"
						string	typestr="filename"	yes
option	"second"	I	"Second DAB file"
						string	typestr="filename"	yes

section "Optional"
option	"memmap"	m	"Memory map input files"
						flag	on
option	"verbosity"	v	"Message verbosity"
						int	default="5"
