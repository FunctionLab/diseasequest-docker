package	"Mat2Txt"
version	"1.0"
purpose	"Text/binary matrix file interconversion"

section "Main"
option	"input"			i	"Input matrix file"
							string	typestr="filename"
option	"output"		o	"Output matrix file"
							string	typestr="filename"

section "Optional"
option	"verbosity"		v	"Message verbosity"
							int	default="5"
